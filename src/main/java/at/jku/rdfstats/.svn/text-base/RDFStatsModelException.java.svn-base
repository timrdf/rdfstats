package at.jku.rdfstats;

/**
 * @author dorgon
 *
 */
public class RDFStatsModelException extends Exception {
	private static final long serialVersionUID = -7327550556342706324L;

	/**
	 * 
	 */
	public RDFStatsModelException() {
		super();
	}

	/**
	 * @param message
	 * @param cause
	 */
	public RDFStatsModelException(String message, Throwable cause) {
		super(message, cause);
	}

	/**
	 * @param message
	 */
	public RDFStatsModelException(String message) {
		super(message);
	}

	/**
	 * @param cause
	 */
	public RDFStatsModelException(Throwable cause) {
		super(cause);
	}

}
